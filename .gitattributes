# Set the default behavior, in case people don't have core.autocrlf set.
* text=false

# Declare files line endings on checkout.
*.sql text eol=lf
*.crt text eol=lf
*.csr text eol=lf
*.key text eol=lf
*.p12 text eol=lf
*.pem text eol=lf
*.cnf text eol=lf
*.txt text eol=lf
*.sh text eol=lf
*.xml text eol=lf
*.XML text eol=lf
*.properties text eol=lf
*.csv text eol=lf
*.yaml text eol=lf
.bash_profile text eol=lf
sshd_config text eol=lf
Dockerfile text eol=lf

# Denote all files that are truly binary and should not be modified.
*.zip binary
*.jks binary
*.bin binary
*.tar binary